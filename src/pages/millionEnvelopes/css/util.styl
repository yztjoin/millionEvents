$origin = #FF7A07
flexPst(jtf, algin)
  display flex
  justify-content jtf
  align-items algin
pst(pst)
  position pst
  if(arguments[1]) {
    top arguments[1]
  }
  if(arguments[2]) {
    right arguments[2]
  }
  if(arguments[3]) {
    bottom arguments[3]
  }
  if(arguments[4]) {
    left arguments[4]
  }
wh(w)
  width w
  if (!!arguments[1])
    height arguments[1]
font(c, s, w)
  color c
  font-size s
  font-weight w
